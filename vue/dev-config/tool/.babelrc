{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-0",
    "flow"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-runtime",
    "async-to-promises"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ]
    }
  }
}